	<groupId>taroko</groupId>
	<artifactId>taroko-parent</artifactId>
	<version>1.0.0</version>
	<packaging>pom</packaging>
	<modules>
		<module>taroko-common</module>
		<module>taroko-authintf</module>
		<module>taroko-authbatch</module>
		<module>taroko-batch</module>
		<module>taroko-web</module>
