export OBJECT_MODE=64
export TMP=/tmp
export TMPDIR=$TMP
export EDITOR=vi

export JAVA_TOOL_OPTIONS=-Dfile.encoding=UTF-8
export PROJ_HOME=/cr/ecs
export JAVA_HOME=/usr/java8_64

export PROJ_ETC=$PROJ_HOME/etc
export PROJ_MEDIA=$PROJ_HOME/media

export PATH="$JAVA_HOME/bin:.:/usr/sbin:/usr/local/bin:/bin:/usr/bin:$PROJ_HOME/etc:$PROJ_HOME/shell"
set -o emacs
export PS1=`whoami`:'${PWD}>'


umask 000
alias ls='ls --color=auto'
unalias ls

cd $PROJ_HOME/log

export LANG=ZH_TW.UTF-8
export LC_CTYPE=ZH_TW.UTF-8
export LC_ALL=ZH_TW.UTF-8

